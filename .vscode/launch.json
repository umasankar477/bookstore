{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Application",
      "request": "launch",
      "mainClass": "customer.capdemo.Application",
      "projectName": "capDemo"
    },
    {
      "type": "java",
      "name": "Application",
      "request": "launch",
      "mainClass": "customer.cap4.Application",
      "projectName": "cap4"
    },
    {
      "name": "Run cap2-1",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}/cap2",
      "runtimeExecutable": "cds",
      "args": [
        "serve",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/cap2/.env1",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/cap2\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}